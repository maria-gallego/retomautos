<script>
    // Initialize and add the map
    function initMap() {
        // Retomautos Selection
        var retomautos_selection = {lat: 4.688071, lng: -74.047168};
        var map_selection = new google.maps.Map(
            document.getElementById('map-selection'), {zoom: 17, center: retomautos_selection});
        var marker = new google.maps.Marker({position: retomautos_selection, map: map_selection});

        // Retomautos Casa
        var retomautos_house = {lat: 4.689062, lng: -74.045493};
        var map_house = new google.maps.Map(
            document.getElementById('map-house'), {zoom: 17, center: retomautos_house});
        var marker = new google.maps.Marker({position: retomautos_house, map: map_house});
    }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAD7KUaG-hvw1jIpCx12Wp4M4y7Vw1oTdI&callback=initMap">
</script>
