<!--Visitanos-->
<div class="bg-light pb-4">
  <div class="pt-5 pb-4">
    <h2 class="text-center font-weight-bold">Visítanos</h2>
  </div>
  <div class="container">
    <div class="row">
      <!--Column 1-->
      <div class="col-sm">
        <div class="card">
          <div class="card-body">
            <h5 class="card-title">Retomautos Selection</h5>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
            <div id="map-selection" style="height: 400px; width: 100%"></div>
            <div class="container">
              <div class="row pt-3 text-center">
                <div class="col-sm pb-3 pb-md-0">
                  <a href="https://goo.gl/maps/BT3TerDdVfRPZEPA7" title="Google Maps">
                    <%= image_tag 'google_maps.png', alt: 'Logo Google Maps', style: 'height: 30px;' %>
                    &nbsp;
                    <span>Llévame con Maps</span>
                  </a>
                </div>
                <div class="col-sm">
                  <a href="https://www.waze.com/ul?place=ChIJIckC18ubP44RM6cCBsRE_0w&ll=4.68803390%2C-74.04714040&navigate=yes" title="Waze">
                    <%= image_tag 'waze_logo.png', alt: 'Logo Waze', style: 'height: 30px;' %>
                    &nbsp;
                    <span>Llévame con Waze</span>
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!--END Column 1-->

      <!--Column 2 -->
      <div class="col-sm">
        <div class="card">
          <div class="card-body">
            <h5 class="card-title">Retomautos AyG S.A.S.</h5>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
            <div id="map-house" style="height: 400px; width: 100%"></div>
            <div class="container">
              <div class="row pt-3 text-center">
                <div class="col-sm pb-3 pb-md-0">
